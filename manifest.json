{
  "manifest_version": 2,

  "name": "MLB Redzone",
  "description": "Extension for MLB TV to change game based on fantasy players/team interest",
  "version": "1.0",
  "browser_action": {
    "default_popup": "options/options.html"
  },
  "permissions": [
    "activeTab",
    "http://lwsa.mlb.com/*"
  ],
	"background":{
		"scripts":["jquery-3.2.1.min.js", "gamechecker.js"],
    "persistent": true
	}
}
