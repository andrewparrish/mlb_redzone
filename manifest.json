{
  "manifest_version": 2,

  "name": "MLB Redzone",
  "description": "Extension for MLB TV to change game based on fantasy players/team interest",
  "version": "1.0",
  "browser_action": {
    "default_popup": "options/options.html"
  },
  "permissions": [
    "activeTab",
    "storage",
    "*://*/*",
    "tabs"
  ],
  "background":{
    "scripts":[
      "jquery-3.2.1.min.js",
      "models/priority_list.js",
      "services/game_checker.js",
      "injections/scrape_current_games.js",
      "injections/scrape_current_game.js",
      "gamechecker.js"
    ],
    "persistent": true
  },
  "web_accessible_resources": [
    "check_game.js",
    "injections/scrape_current_games.js",
    "injections/scrape_current_game.js"
  ]
}
